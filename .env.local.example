# Supabase Configuration (Auth & Database only)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# Supabase Service Role Key (Server-side only)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# AWS S3 Configuration (Video Storage)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key
AWS_S3_BUCKET_NAME=speaking-coach-videos

# Google Gemini API Configuration
# Get your API key from: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your-gemini-api-key-here

# External AI Model Configuration
# URL for the external AI video analysis service
EXTERNAL_AI_API_URL=http://10.59.19.205:9000/analyze/

# Timeout for external AI requests in milliseconds (default: 120000 = 2 minutes)
EXTERNAL_AI_TIMEOUT=240000

# Feature flag to enable/disable external AI integration (true/false)
EXTERNAL_AI_ENABLED=true
